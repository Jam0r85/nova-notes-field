{
  "name": "jam0r85/nova-notes-field",
  "description": "This Laravel Nova package adds a notes field to Nova's arsenal of fields.",
  "keywords": [
    "laravel",
    "nova",
    "optimistdigital",
    "field"
  ],
  "authors": [
    {
      "name": "James Morris",
      "email": "jam0r@me.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Jam0r85\\NovaNotesField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jam0r85\\NovaNotesField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
